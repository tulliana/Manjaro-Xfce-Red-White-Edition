[action0]
name=Man Sayfaları
pattern=#
command=exo-open --launch TerminalEmulator man %s
regex=false

[action1]
name=Wikipedia
pattern=!w
command=exo-open --launch WebBrowser https://en.wikipedia.org/wiki/%u
regex=false

[action2]
name=Konsolda Çalıştır
pattern=!
command=exo-open --launch TerminalEmulator %s
regex=false

[action3]
name=URL aktif
pattern=^(file|http|https):\\/\\/(.*)$
command=exo-open \\0
regex=true

